//This will contain the details of the advisor (employee name, id, etc.)
import java.util.Random;

class Employee
{
  protected int[] employeeID;
  protected int[] customerID;
  private String employeeName;
  private Random rand;

  Employee()
  {
    rand = new Random();
    customerID = new int[6];
    employeeID = new int[6];
    employeeName = "";
  }

  void setEmployeeID(int[] ID);
  {
    this.ID = employeeID;
    for(int i = 0; i < employeeID.length; i++)
    {
      employeeID[i] = rand.nextInt(10);
    }

    System.out.println("Employee ID: " + employeeID.toString());
  }

  void setCustomerID(int[] cID)
  {
    this.cID = customerID;
    for(int i = 0; i < customerID.length; i++)
    {
      customerID[i] = rand.nextInt(10);
    }

    System.out.println("Customer ID: " + customerID.toString());
  }
}

public class Employee_Test
{
  public static void main(String[] args)
  {
      Employee e = new Employee();
      e.setEmployeeID();
      e.setCustomerID();
  }
}
